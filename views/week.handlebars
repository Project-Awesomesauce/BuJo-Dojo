      <div class="row weekly-view" id="day-one-div">

      	<span id="day-one-span"></span>

      	<ul class="fa-ul">
          {{#each tasks}}
	        {{#unless this.completed}}
	          <li data-category="{{category}}" data-completed="{{completed}}">
	            <span class="list-item">{{item}}</span>
	              <button data-id="{{id}}" class="edit-item" type="submit">Edit</button>
	              <button data-id="{{id}}" class="destroy-item" type="submit">X</button>
	          </li>
	        {{/unless}}
	      {{/each}}
	    </ul>

